<zk>
	<!-- id generator for selenium testing -->
	<system-config>
		<id-generator-class>org.adempiere.webui.AdempiereIdGenerator</id-generator-class>
	</system-config>

	<!-- turn off uuid reuse for selenium testing -->
	<library-property>
		<name>org.zkoss.zk.ui.uuidRecycle.disabled</name>
		<value>true</value>
	</library-property>	
</zk>
